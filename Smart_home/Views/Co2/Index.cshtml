@model IEnumerable<Smart_home.Models.Co2>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Cas)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Eco2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Etvoc)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Raw6)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Raw10)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Odpor)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Cas)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Eco2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Etvoc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Raw6)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Raw10)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Odpor)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
